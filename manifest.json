{
  "manifest_version": 3,
  "name": "Overlay Extension",
  "version": "1.0",
  "description": "An extension that overlays the window.",
  "action": {
    "default_popup": "popup.html"

  },
  "permissions": ["scripting", "activeTab","storage","tabs","downloads","sidePanel","webNavigation"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["src/index.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
